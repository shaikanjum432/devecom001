{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfecomdev001"
		},
		"LS_SqlServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SqlServer'"
		},
		"LS_SQL_PAR_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_SQL_PAR'"
		},
		"LinkedintegrationRuntime1_properties_typeProperties_linkedInfo_resourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/1d0dee26-79e1-43df-875b-593f4a0f5a58/resourcegroups/ResourceGroup_My/providers/Microsoft.DataFactory/factories/ADFOneMy/integrationruntimes/integrationRuntime1new"
		},
		"LS_SqlServer_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "LAPTOP-3FGB03GR\\SQLEXPRESS"
		},
		"LS_SqlServer_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "ecom_database"
		},
		"LS_SqlServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa"
		},
		"LS_SQL_PAR_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlssiecomdev001.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfecomdev001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_SQL_PAR_accountKey": {
						"value": "[parameters('LS_SQL_PAR_accountKey')]"
					},
					"LS_SqlServer_password": {
						"value": "[parameters('LS_SqlServer_password')]"
					},
					"LinkedintegrationRuntime1_properties_typeProperties_linkedInfo_resourceId": {
						"value": "[parameters('LinkedintegrationRuntime1_properties_typeProperties_linkedInfo_resourceId')]"
					},
					"LS_SQL_PAR_properties_typeProperties_url": {
						"value": "[parameters('LS_SQL_PAR_properties_typeProperties_url')]"
					},
					"LS_SqlServer_properties_typeProperties_server": {
						"value": "[parameters('LS_SqlServer_properties_typeProperties_server')]"
					},
					"LS_SqlServer_properties_typeProperties_database": {
						"value": "[parameters('LS_SqlServer_properties_typeProperties_database')]"
					},
					"LS_SqlServer_properties_typeProperties_userName": {
						"value": "[parameters('LS_SqlServer_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}